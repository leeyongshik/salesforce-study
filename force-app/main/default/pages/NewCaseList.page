<apex:page controller="NewCaseListController" tabStyle="Case">
    <apex:pageBlock title="New Cases">
        <apex:repeat value="{!newCases}" var="case">
            <apex:outputLink value="/{!case.Id}">{!case.CaseNumber}</apex:outputLink><Br/>
        </apex:repeat>
    </apex:pageBlock>
</apex:page>